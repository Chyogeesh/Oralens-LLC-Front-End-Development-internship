npx create-react-app healthcare-dashboard
cd healthcare-dashboard
npm start
pip install flask
mkdir templates uploads
python app.py
npm install @mui/material @emotion/react @emotion/styled
pip install flask-cors
pip install flask-sqlalchemy
docker build -t healthcare-flask .
docker run -p 5000:5000 healthcare-flask
docker build -t healthcare-dashboard .
docker run -p 3000:3000 healthcare-dashboard
pip install -r requirements.txt
npm test
